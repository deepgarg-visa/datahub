#This file is for reference, the actual data comes from artifactory

INDEX_PREFIX=vdc_dev
DATAHUB_GMS_HOST=localhost
DATAHUB_GMS_PORT=8080
DATAHUB_DB_NAME=datahub
DATAHUB_PRECREATE_TOPICS="false"
DATAHUB_SERVER_TYPE="quickstart"
DATAHUB_TELEMETRY_ENABLED="false"
DATAHUB_UPGRADE_HISTORY_KAFKA_CONSUMER_GROUP_ID="generic-duhe-consumer-job-client-gms"
DATAHUB_USAGE_EVENT_NAME=${INDEX_PREFIX}_DataHubUsageEvent_v1
EBEAN_DATASOURCE_DRIVER="com.mysql.cj.jdbc.Driver"
ELASTICSEARCH_INDEX_BUILDER_MAPPINGS_REINDEX="true"
ELASTICSEARCH_INDEX_BUILDER_SETTINGS_REINDEX="true"
ELASTICSEARCH_PORT="9243"
ELASTICSEARCH_PROTOCOL="https"
ELASTICSEARCH_USE_SSL="true"
ENTITY_REGISTRY_CONFIG_PATH="../resources/entity-registry.yml"
ENTITY_SERVICE_ENABLE_RETENTION="true"
ES_BULK_REFRESH_POLICY="WAIT_UNTIL"
FAILED_METADATA_CHANGE_EVENT_NAME=${INDEX_PREFIX}_FailedMetadataChangeEvent_v4
FAILED_METADATA_CHANGE_PROPOSAL_TOPIC_NAME=${INDEX_PREFIX}_FailedMetadataChangeProposal_v1
GRAPH_SERVICE_DIFF_MODE_ENABLED="true"
GRAPH_SERVICE_IMPL="neo4j"
KAFKA_BROKER_PORT=9092
SSL_TRUSTSTORE_FILE=../certs/cacerts
SSL_TRUSTSTORE_PASSWORD=changeit
KEYSTORE_FILE=../certs/cacerts
KEYSTORE_PASSWORD=changeit
MAE_CONSUMER_ENABLED="true"
MCE_CONSUMER_ENABLED="true"
METADATA_AUDIT_EVENT_NAME=${INDEX_PREFIX}_MetadataAuditEvent_v4
METADATA_CHANGE_EVENT_NAME=${INDEX_PREFIX}_MetadataChangeEvent_v4
METADATA_CHANGE_LOG_TIMESERIES_TOPIC_NAME=${INDEX_PREFIX}_MetadataChangeLog_Timeseries_v1
METADATA_CHANGE_LOG_VERSIONED_TOPIC_NAME=${INDEX_PREFIX}_MetadataChangeLog_Versioned_v1
METADATA_CHANGE_PROPOSAL_TOPIC_NAME=${INDEX_PREFIX}_MetadataChangeProposal_v1
METADATA_SERVICE_AUTH_ENABLED="true"
MYSQL_PORT=3306
PE_CONSUMER_ENABLED="true"
PLATFORM_EVENT_TOPIC_NAME=${INDEX_PREFIX}_PlatformEvent_v1
SCHEMA_REGISTRY_PORT=8081
SESSION_TOKEN_DURATION_MS=900000
#SPRING_KAFKA_SASL_MECHANISM="GSSAPI"
#SPRING_KAFKA_SECURITY_PROTOCOL="SASL_SSL"
UI_INGESTION_ENABLED="true"


